<div class="actions">
    <a href="/admin/problem/to_excel" id="to_excel">导出 Excel</a>
    <a href="/admin/problem/new" id="to_excel">新建题目</a>
</div>
<table class="problem-table">
    <thead>
        <th>ID</th>
        <th>标题</th>
        <th>日期</th>
        <th>操作</th>
    </thead>
    <tbody>
        <% for (let i = 0; i < problems.length; i++) { 
            const line = problems[i];%>
        <tr>
            <td><%= line.id %></td>
            <td><%= line.title %></td>
            <td><%= new Date(line.createdAt).toISOString() %></td>
            <td>
                <button type="button" onclick="deleteProblem(<%= line.id %>)" id="">删除</button>
                <a href="/admin/problem/modify/<%= line.id %>">修改</button>
            </td>
        </tr>
        <% } %>
    </tbody>
</table>
<script src="/javascripts/adminProblem.js"></script>